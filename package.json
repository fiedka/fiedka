{
  "name": "fiedka",
  "productName": "fiedka",
  "version": "1.3.4",
  "description": "Fiedka the Firmware Editor",
  "main": ".webpack/main",
  "scripts": {
    "storybook": "storybook-server --ci --port 6006",
    "start": "GOPATH=$(pwd) GOROOT=$(go env GOROOT) electron-forge start",
    "package": "GOPATH=$(pwd) GOROOT=$(go env GOROOT) electron-forge package",
    "make": "GOPATH=$(pwd) GOROOT=$(go env GOROOT) electron-forge make",
    "publish": "GOPATH=$(pwd) GOROOT=$(go env GOROOT) electron-forge publish",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "fmt": "prettier --write src/*.jsx src/**/*.jsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiedka/fiedka.git"
  },
  "keywords": [
    "App",
    "Firmware",
    "x86",
    "AMD",
    "Intel",
    "Editor",
    "UEFI"
  ],
  "author": {
    "name": "Daniel Maslowski",
    "email": "info@orangecms.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/orangecms/utk-web/issues"
  },
  "homepage": "https://fiedka.app/",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "fiedka"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "fiedka",
              "name": "fiedka"
            }
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@electron-forge/publisher-github": "^6.0.0-beta.63",
    "@fiedka/golang-wasm-async-loader": "^4.0.0",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addons": "^6.1.17",
    "@storybook/react": "^6.1.17",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.6.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.0.0",
    "electron": "13.2.2",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.22.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "jest": "^26.6.3",
    "node-loader": "^2.0.0",
    "prettier": "^2.5.1",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@coalmines/indui": "^1.6.0",
    "classnames": "^2.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-sudo": "^4.0.12",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-jsx": "^4.0.0",
    "use-file-picker": "^1.3.0"
  }
}
