    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-storybook": "^0.8.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "jest": "^29.7.0",
    "node-loader": "^2.0.0",
    "prettier": "^3.3.3",
    "storybook": "^8.2.9",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
