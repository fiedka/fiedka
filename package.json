{
  "name": "fiedka",
  "productName": "fiedka",
  "version": "1.5.2",
  "description": "Fiedka the Firmware Editor",
  "main": ".webpack/main",
  "scripts": {
    "storybook": "TS_NODE_PROJECT='./.storybook/tsconfig.json' storybook dev --ci --port 6006",
    "start": "GOPATH=$(pwd) GOROOT=$(go env GOROOT) electron-forge start",
    "package": "GOPATH=$(pwd) GOROOT=$(go env GOROOT) electron-forge package",
    "make": "GOPATH=$(pwd) GOROOT=$(go env GOROOT) electron-forge make",
    "publish": "GOPATH=$(pwd) GOROOT=$(go env GOROOT) electron-forge publish",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "fmt": "prettier --write src/*.jsx src/**/*.jsx",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiedka/fiedka.git"
  },
  "keywords": [
    "App",
    "Firmware",
    "x86",
    "AMD",
    "Intel",
    "Editor",
    "UEFI"
  ],
  "author": {
    "name": "Daniel Maslowski",
    "email": "info@orangecms.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/orangecms/utk-web/issues"
  },
  "homepage": "https://fiedka.app/",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-webpack": "^6.4.2",
    "@electron-forge/publisher-github": "^6.4.2",
    "@fiedka/golang-wasm-async-loader": "^4.0.0",
    "@storybook/addon-actions": "^7.5.1",
    "@storybook/addons": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-webpack5": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "electron": "^27.0.2",
    "eslint": "^8.52.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.4.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.6.15",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "jest": "^29.7.0",
    "node-loader": "^2.0.0",
    "prettier": "^3.0.3",
    "storybook": "^7.5.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@coalmines/indui": "^1.6.0",
    "@reduxjs/toolkit": "^1.9.7",
    "classnames": "^2.3.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-sudo": "^4.0.12",
    "joi": "^17.11.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
